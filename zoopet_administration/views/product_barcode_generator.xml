<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="zoopet_administration.view_barcode_generator" model="ir.ui.view">
            <field name="name">view.barcode.generator</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="barcodes_generator_product.view_product_template_form"/>
            <field name="arch" type="xml">
                 <xpath expr="//field[@name='barcode']" position="replace">
                    <field name="barcode"></field>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
