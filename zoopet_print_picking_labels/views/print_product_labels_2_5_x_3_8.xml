                <t t-foreach="docs" t-as="picking">
                    <t t-foreach="picking.move_lines" t-as="move_line">
                        <t t-if="move_line.to_print">
                            <t t-foreach="int(move_line.quantity_done)" t-as="">
                            <div class="page">
                                <t t-call="zoopet_print_picking_labels.report_simple_label_petpoint">
                                    <t t-set="product" t-value="move_line.product_id"/>
                                     <t t-set="moveline" t-value="move_line"/>
                                </t>
                            </div>
                        </t>
                        </t>
                    </t>
                </t>
            </t>
        </template>
    </data>
    <!-- FIN DESDE PICKING -->
    <!-- DESDE PRODUCT TEMPLATE -->
    <data>
        <report id="report_labels_25_23_zoopet_product_template"
                string="Etiquetas 2.5 x 3.8 Zoopet"
                model="product.product"
                report_type="qweb-pdf"
                name="zoopet_print_picking_labels.report_labels_25_23_document_zoopet_product_template"
                print_report_name="'Producto - %s' % (object.name)"
        />
