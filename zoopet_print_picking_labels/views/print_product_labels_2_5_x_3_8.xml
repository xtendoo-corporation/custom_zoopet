            </t>
        </template>
    </data>

     <data>
        <report id="report_labels_25_23_petpoint_product_template"
                string="Etiquetas 2.5 x 3.8 Petpoint"
                model="product.product"
                report_type="qweb-pdf"
                name="zoopet_print_picking_labels.report_labels_25_23_document_petpoint_product_template"
